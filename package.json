{
  "name": "angular-base-template",
  "version": "0.0.1-SNAPSHOT",
  "description": "Angular Base Template Description",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "ng run angular-base-template:serve-ssr --proxy-config proxy.conf.json",
    "dev:ssr": "ng run angular-base-template:serve-ssr",
    "serve:ssr": "node dist/angular-base-template/server/main.js",
    "build:ssr": "ng build && ng run angular-base-template:server",
    "prerender": "ng run angular-base-template:prerender",
    "release:dry": "RUN_DRY=true npx zx ./scripts/release.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.2.4",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.2.4",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/platform-server": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@nguniversal/express-engine": "^15.2.0",
    "daisyui": "^2.19.0",
    "express": "^4.15.2",
    "ngx-spinner": "^15.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular/cli": "~15.1.2",
    "@angular/compiler-cli": "^15.1.0",
    "@nguniversal/builders": "^15.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.18.41",
    "autoprefixer": "^10.4.7",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "~4.9.4"
  }
}
